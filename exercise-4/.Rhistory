is_twice_as_long <- function(x, y) {
return (x >= y)
}
is_twice_as_long <- function(x, y) {
return (x >= 2*y)
}
number <- is_twice_as_long(a, bcd)
number <- is_twice_as_long("a", "bcd")
is_twice_as_long <- function(x, y) {
return (nchar(x) >= nchar(2*y))
}
number <- is_twice_as_long("a", "bcd")
is_twice_as_long <- function(x, y) {
return (nchar(x) >= 2 * nchar(y))
}
number <- is_twice_as_long("a", "bcd")
is_twice_as_long("as","n")
